<!--                    BOOK TABLE COUNT ATTRIBUTES                -->
<!--                    Attributes for the <book-table-count> element
                                                                   -->
<!ENTITY % book-table-count-atts
            "%jats-common-atts;                                       
             count      NMTOKEN                           #REQUIRED" >


<!--                    BOOK WORD COUNT ATTRIBUTES                 -->
